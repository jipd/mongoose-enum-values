{
  "name": "@jipd/mongoose-enum-values",
  "version": "1.0.0",
  "description": "Retrieve enum values for mongoose schemas",
  "main": "dist/index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
      "test": "echo \"Error: no test specified\" && exit 1",
      "clean": "rm -rf dist && mkdir dist",
      "build-babel": "babel ./lib -d ./dist -s",
      "build": "npm run clean && npm run build-babel",
      "dev": "watch 'npm run build' ./lib",
      "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jipd/mongoose-enum-values.git"
  },
  "keywords": [
    "Mongoose",
    "Enum"
  ],
  "author": "Nathan Bryant",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jipd/mongoose-enum-values/issues"
  },
  "homepage": "https://github.com/jipd/mongoose-enum-values#readme",
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/node": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.13.9",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.13.9",
    "@babel/runtime": "^7.13.9",
    "babel-preset-minify": "^0.5.1",
    "watch": "^1.0.2"
  }
}
